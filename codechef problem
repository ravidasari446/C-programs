#include <stdio.h>

int main(void) {
    int p1,p2,p3,p4,ans=0;
    scanf("%d%d%d%d",&p1,&p2,&p3,&p4);
    if(p1>9) {
    ans++;
    }
    if(p2>9){
    ans++;
    }
    if(p3>9){
    ans++;
    }
    if(p4>9){
    ans++;
    }
    printf("%d",ans);
	return 0;
}
INPUT: 
       1 3 5 10
OUTPUT: 
         1
