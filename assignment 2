#include <stdio.h>

int main()
{
    int i,j,x;
    i=x;
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=i;j++)
        {
            printf("*");
            x++;
        }
        printf("\n");

    }
    for(i=4;i>=1;i--)
    {
        for(j=1;j<=i;j++)
        {
            printf("*");
            x--;
        }
        printf("\n");
    }

    return 0;
}
OUTPUT:
       *
       **
       ***
       ****
       *****
       ****
       ***
       **
       *
